@using UT.SL.Helper
@using UT.SL.Security
@model UT.SL.Data.LINQ.SocialGroup
<div class="">
    <div class="row">
        @Html.Action("AddMember", "SocialGroup", new { area = "Admin", Id = Model.Id, largepic = 1 })
    </div>
    <div class="row">
        <div class="col-md-12" id="members@(Model.Id)">
            @Html.Action("GetMembersLargePic", "SocialGroup", new { area = "Admin", Id = Model.Id })
        </div>
    </div>
    <div class="row">
        <div class="col-md-2 col-md-offset-2" style="margin-top:20px">
            <a href="javascript:void(0);" class="btn btn-default closeWindow" onclick="$(this).closest('.ui-dialog-content').dialog('close');">@UT.SL.Model.Resource.App_Common.CloseWindow</a>
        </div>
    </div>
    <script>
        $(function () {
            $.InitialComponents();
        });
    </script>
</div>
