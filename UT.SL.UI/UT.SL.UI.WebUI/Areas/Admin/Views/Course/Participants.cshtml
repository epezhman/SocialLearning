@model UT.SL.Data.LINQ.Course
@{
    ViewBag.Title = Model.Title;
}
@section scripts {
    <script type="text/javascript">
        $(function () {
            $("#arrowheader").show();
            highLight(@((int)UT.SL.Model.Enumeration.MenuType.MyCourses), @Model.Id, true, false);
        });
    </script>
    <script src="@Url.Content("~/Scripts/highcharts/js/highcharts.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/highcharts/js/highcharts-more.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/highcharts/js/modules/exporting.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/highcharts/js/modules/data.js")" type="text/javascript"></script>
}
@section TopLinks{
    @Html.Action("TopMenuLinks", "Home", new { area = "", menuType = (int)UT.SL.Model.Enumeration.MenuType.MyCourses, extra = Model.Id, rank = (ViewBag.LinkRank != null ? (int)ViewBag.LinkRank : 0), id = Model.Id })
}
<div class="">
    <div class="row">
        <div class="col-md-6" id="mainPortal">
        </div>
        <div class="col-md-6 panelCompo" id="stufftobemoved">
            @Html.Action("GetPanel", new { Model, filter = ViewBag.Filter })
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div id="userPortfolio">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div id="activities">
            </div>
        </div>
    </div>
</div>
